#--------------------------------------------------------
# asdf
#--------------------------------------------------------
[ `uname -m` = 'arm64' ] && . /opt/homebrew/opt/asdf/libexec/asdf.sh

#--------------------------------------------------------
# jump
#--------------------------------------------------------
eval "$(jump shell)"

#--------------------------------------------------------
# alias
#--------------------------------------------------------
alias la="ls -aFG"
alias ls="ls -FG"
alias ll="ls -alFG"
alias dc="docker-compose"
alias do="docker"
#--------------------------------------------------------
# path
#--------------------------------------------------------
export LC_ALL="ja_JP.UTF-8"
#--------------------------------------------------------
# functions
#--------------------------------------------------------
export PATH="/opt/homebrew/opt/openssl@3/bin:$PATH"
